{
	"name": "codetracker",
	"displayName": "CodeTracker",
	"description": "CodeTracker tracks your time spent in different projects and files in Visual Studio Code.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codetracker.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "CodeTracker",
				"command": "codetracker.activate",
				"title": "Activate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"lint:fix": "eslint src --ext ts --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"typescript": "^4.9.3"
	},
	"dependencies": {
		"prettier": "^2.8.1"
	}
}
